<article class="sidebar">
    <h1 class="photolist-title">Photos on display</h1>
    <div class="card">      
        <% if (photos.length > 0) { %>
        <% for (i=0; i< photos.length; i++) { %>
          <a class="lightbox" href=""/photoalbum/<%= photos[i] %>"">
          <img src="/photoalbum/<%= photos[i] %>" alt="Photo <%= i %>">
          </a>        
        <% } %>
        <% } %>  
    </div>
</article>